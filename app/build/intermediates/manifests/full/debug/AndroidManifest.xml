<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.au.allotedv6"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.Read_EXTERNAL_STORAGE" />

    <!-- You should set 'versionCode' and 'versionName' in QBSettings class. -->
    <!-- It's library project. It doesn't need any manifest settings. -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.flash" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/resoicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.au.allotedv6.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.au.allotedv6.Splash"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.au.allotedv6.ViewFreeslots"
            android:label="ViewFreeslots"
            android:parentActivityName="com.au.allotedv6.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.au.allotedv6.contributers"
            android:label="@string/title_activity_contributers"
            android:parentActivityName="com.au.allotedv6.MainActivity"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustPan" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.au.allotedv6.MainActivity" />
        </activity>
        <activity
            android:name="com.au.allotedv6.Login_Activity"
            android:label="@string/title_activity_login_"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.au.allotedv6.Seleectslots"
            android:label="@string/title_activity_seleectslots"
            android:parentActivityName="com.au.allotedv6.MainActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.au.allotedv6.MainActivity" />
        </activity>
        <activity
            android:name="com.au.allotedv6.getfile"
            android:label="Getfile" >
        </activity>
        <activity
            android:name="com.au.allotedv6.Launch"
            android:label="@string/title_activity_launch"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.au.allotedv6.Allotentmain"
            android:label="@string/title_activity_allotentmain"
            android:theme="@style/AppTheme" >
        </activity>
    </application>

</manifest>